{
  "name": "effect-interactive-lab",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "Interactive lab showcasing Effect library's capabilities for building robust TypeScript applications",
  "keywords": [
    "effect",
    "typescript",
    "functional-programming",
    "concurrency",
    "error-handling",
    "streams",
    "demo",
    "interactive",
    "react"
  ],
  "homepage": "https://github.com/your-username/effect-interactive-lab#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/effect-interactive-lab.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/effect-interactive-lab/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Effect Interactive Lab Contributors",
    "url": "https://github.com/your-username/effect-interactive-lab/graphs/contributors"
  },
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\" --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\"",
    "client": "vite",
    "server": "node server/index.js",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc -b --noEmit",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "start": "npm run build && npm run preview"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@tailwindcss/vite": "^4.1.11",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "effect": "^3.16.10",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
